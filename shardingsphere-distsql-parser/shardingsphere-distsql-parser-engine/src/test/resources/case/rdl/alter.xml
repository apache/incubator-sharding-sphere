<sql-parser-test-cases>
    <alter-sharding-table-rule sql-case-id="alter-sharding-table-rule">
        <table name="t_order" start-index="" stop-index="" />
        <resource name="ms_group_0" start-index="" stop-index="" />
        <resource name="ms_group_1" start-index="" stop-index="" />
        <sharding-column name="order_id" start-index="" stop-index="" />
        <type name = "hash_mod" start-index="" stop-index="">
            <propert name ="sharding-count" />
            <generated-key>
                <colum name = "anther_id" />
                <type name = "snowflake">
                    <propert name ="" />
                </type>
            </generated-key>
        </type>
    </alter-sharding-table-rule>

    <alter-sharding-binding-table-rules sql-case-id = "alter-sharding-binding-table-rules">
        <table name="t_order" />
        <table name="t_order_item" />
        <table name="t_order" />
        <table name="t_1" />
        <table name="t_1" />
    </alter-sharding-binding-table-rules>

    <alter-sharding-broadcast-table-rules sql-case-id = "alter-sharding-broadcast-table-rules">
        <table name = "t_1" />
        <table name = "t_2" />
    </alter-sharding-broadcast-table-rules>

    <alter-readwrite-splitting-rule sql-case-id = "alter-readwrite-splitting-rule">
        <resource name="ms_group_0"/>
        <auto-aware-resource name="group_0" />
        <type name = "random">
            <propert name ="read_weight" value = "2:1" />
        </type>
        <resource name="ms_group_1"/>
        <write-resource name="primary_ds" />
        <read-resource>
            <resource name = "replica_ds_0" />
            <resource name = "replica_ds_1" />
        </read-resource>
        <type name = "random" />
    </alter-readwrite-splitting-rule>

    <alter-database-discovery-rule sql-case-id = "alter-database-discovery-rule">
        <rule name = "ha_group_0" >
            <resource name = "resource0" />
            <resource name = "resource1" />
            <type name = "mgr">
                <propert>
                    <name name="groupName"/>
                    <vale name = "92504d5b-6dec"/>
                </propert>
                <propert>
                    <name name="keepAliveCron"/>
                    <vale name = ""/>
                </propert>
            </type>
        </rule>

        <rule name = "ha_group_1" >
            <resource name = "resource2" />
            <resource name = "resource3" />
            <type name = "mgr2">
                <propert>
                    <name name="groupName"/>
                    <vale name = "92504d5b-6dec=2"/>
                </propert>
                <propert>
                    <name name="keepAliveCron"/>
                    <vale name = ""/>
                </propert>
            </type>
        </rule>
    </alter-database-discovery-rule>

    <alter-encrypt-rule sql-case-id = "alter-encrypt-rule">
        <rule name = "t_encrypt">
            <colums>
                <colum>
                    <name name="user_id"/>
                    <plan name="user_plain"/>
                    <cipher name="user_cipher"/>
                    <type name = "AES">
                        <propert>
                            <name name="aes-key-value"/>
                            <vale name = "123456abc"/>
                        </propert>
                    </type>
                </colum>
                <colum>
                    <name name="order_id" />
                    <cipher name="order_cipher"/>
                    <type name = "MD5"/>
                </colum>
            </colums>
        </rule>
    </alter-encrypt-rule>
</sql-parser-test-cases>