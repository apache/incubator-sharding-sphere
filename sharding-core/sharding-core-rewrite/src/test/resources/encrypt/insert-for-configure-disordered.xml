<rewrite-assertions yaml-rule="yaml/encrypt/insert-for-configure-disordered.yaml">

	<rewrite-assertion id="insert_value_for_parameters">
        <input sql="INSERT INTO t_account(account_id, password, amount, status) VALUES (?, ?, ?, ?)" parameters="1, aaa, 1000, OK" />
        <output sql="INSERT INTO t_account(account_id, cipher_password, cipher_amount, cipher_status, password, amount, status) VALUES (?, ?, ?, ?, ?, ?, ?)" parameters="1, encrypt_aaa, encrypt_1000, encrypt_OK, aaa, 1000, OK" />
    </rewrite-assertion>
</rewrite-assertions>
